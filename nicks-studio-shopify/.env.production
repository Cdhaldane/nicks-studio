# Production Environment Variables for GitHub Pages
# These should be added to your GitHub repository secrets

# Supabase Configuration (Required for newsletter)
REACT_APP_SUPABASE_URL=your_production_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# Shopify Configuration (If using shop features)
REACT_APP_SHOPIFY_DOMAIN=https://your-store.myshopify.com
REACT_APP_SHOPIFY_ACCESS_TOKEN=your_shopify_access_token

# MailChimp Configuration (If using legacy mailchimp)
REACT_APP_MAILCHIMP_URL=your_mailchimp_url

# Build Configuration
CI=false
PUBLIC_URL=https://nickolamagnolia.com
